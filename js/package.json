    "test:docker": "docker-compose -f ../../docker-compose.test.yml run --rm js-test",
    "serve": "npm run build && node build/server.js",
    "start": "npm run serve",
    "gcp-build": "node node_modules/puppeteer/install.mjs"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "build/index.js",
